
@{
    ViewBag.Title = "Index";
}

@section Scripts{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/jstree.min.js" defer></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-slimScroll/1.3.8/jquery.slimscroll.min.js" defer></script>
    <script src="/js/ScriptControl/NTControl.js" defer></script>
    <script src="/js/ScriptControl/upload.js" defer></script>
}
<h2>Index</h2>
@Html.Partial("/Areas/Admin/Views/Shared/UploadLayout.cshtml")
<script>
    function runUpload() {
        if (typeof FM != 'undefined') {
            FM.setFolderID(1);
            FM.create();
       
        } else {
            setTimeout(runUpload, 10);
        }
    }
    runUpload();
</script>